  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.ts --max-warnings=0",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier . --write"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.5.0",
