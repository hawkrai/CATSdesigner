    "babel-eslint": "10.1.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "prettier": "2.4.1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint:fix"
